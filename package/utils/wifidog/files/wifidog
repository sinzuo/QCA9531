
config wifidog 'wifidog'
	option gateway_connmax '64'
	option gateway_interface 'br-lan'
	option check_interval '60'
	option client_timeout '5'
	option wifidog_enable '0'
	option gateway_eninterface 'eth1'
	option gateway_hostname 'pf.pifii.com'
	option gateway_httpport '8082'
	option gateway_path '/wifiaaa/'
	list whitelist 'wifi.weixin.qq.com'

config wifidog_deamo 'wifidog_deamo'
	option enable '1'

